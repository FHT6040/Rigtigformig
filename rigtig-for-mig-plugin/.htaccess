# Disable LiteSpeed Server Cache for Rigtig for mig plugin
# This prevents server-level caching from minifying/combining our JavaScript files

<IfModule LiteSpeed>
    # Disable cache lookup for all files in this directory
    CacheLookup off

    # Disable cache storage
    CacheDisable on
</IfModule>

# Alternative headers for non-LiteSpeed servers
<FilesMatch "\.(js|css)$">
    Header set Cache-Control "no-cache, no-store, must-revalidate"
    Header set Pragma "no-cache"
    Header set Expires 0
</FilesMatch>
